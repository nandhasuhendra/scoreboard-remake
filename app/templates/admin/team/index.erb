<% tables = Terminal::Table.new do |table| %>
  <% table.headings = ['Id', 'Username', 'Password', 'Token', 'Admin', 'Created at', 'Updated at'] %>
  <% @resources.each do |resource| %>
    <% table.add_row [resource.id, resource.username, resource.password_digest, resource.token, resource.is_admin, resource.created_at, resource.updated_at] %>
  <% end %>
<% end %>

<%= tables %>
